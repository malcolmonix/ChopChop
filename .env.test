# Test Environment Variables for E2E Tests
# This file is used when running Playwright E2E tests

# Server URL
NEXT_PUBLIC_SERVER_URL=http://localhost:4000/

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=test-secret-for-e2e-tests-only

# Firebase Web Config (Test/Mock values)
# For E2E tests, we use mock/test Firebase configuration
NEXT_PUBLIC_FIREBASE_API_KEY=test-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=test-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=test-project
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=test-project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=G-ABCDEF123

# MenuVerse Integration (Test)
NEXT_PUBLIC_MENUVERSE_API_KEY=test-menuverse-key

# Google Maps (Test - use fallback)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=
NEXT_PUBLIC_USE_FALLBACK_GEOCODING=true
